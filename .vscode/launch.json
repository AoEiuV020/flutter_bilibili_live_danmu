    "version": "0.2.0",
    "configurations": [
        {
            "name": "bilibili_live_danmu",
            "cwd": "apps/bilibili_live_danmu",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "bilibili_live_danmu(args)",
            "cwd": "apps/bilibili_live_danmu",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-entrypoint-args=launch-auto_start=true",
                "--dart-entrypoint-args=server-backend_url=http://127.0.0.1:8080",
                "--web-browser-flag=--disable-web-security",
                "--web-port=9080",
                "--web-hostname=127.0.0.1",
                "--web-launch-url=http://127.0.0.1:9080/?launch-auto_start=true&server-backend_url=http://127.0.0.1:8080",
            ]
        },
        {
            "name": "bilibili_live_danmu_proxy(args)",
            "cwd": "apps/bilibili_live_danmu_proxy",
            "request": "launch",
            "type": "dart",
            "args": [
                "--config=../bilibili_live_danmu/assets/config.properties",
            ]
        }
    ]
}